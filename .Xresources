!--------------------xdefaults
URxvt.font: xft:Monaco for Powerline:pixelsize=15,\
    xft:symbola,\
    xft:symbola-free,\
    xft:Symbola,\
    xft:Symbola Free

urxvt*termName: rxvt
urxvt*scrollBar: false
urxvt*matcher.button: 1
urxvt.boldFont:
Xft*dpi: 96
Xft*antialias: true
Xft*hinting: true
Xft*hintstyle: hintfull
Xft*rgba: rgb
URxvt*geometry: 85x20
*internalBorder: 23
URxvt*fading: 0
URxvt*tintColor: #ffffff
URxvt*shading: 0
URxvt*inheritPixmap: False

! This section enables copy and pasting from the clipboard
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.keysym.Control-Meta-c: builtin-string:
URxvt.keysym.Control-Meta-v: builtin-string:
URxvt.iso14755 : false
URxvt.iso14755_52: false

! special
*.foreground:   #c5c8c6
*.background:   #333232
*.cursorColor:  #c5c8c6

! black
*.color0:       #585c69
*.color8:       #585c69

! red
*.color1:       #cab0a1
*.color9:       #cab0a1

! green
*.color2:       #9f8578
*.color10:      #9f8578

! yellow
*.color3:       #986c5b
*.color11:      #986c5b

! blue
*.color4:       #808178
*.color12:      #808178

! magenta
*.color5:       #979892
*.color13:      #979892

! cyan
*.color6:       #cab0a1
*.color14:      #cab0a1

! white
*.color7:       #707880
*.color15:      #707880

rofi.font:            NotoSans-Medium 30
rofi.lines: 3
rofi.eh: 2
rofi.padding: 400
rofi.show-match: false
rofi.width: 20
rofi.fullscreen: true
rofi.opacity: 85
rofi.bw: 0
rofi.separator-style: none
rofi.hide-scrollbar: true

!                  'background', 'border', 'separator'
rofi.color-window: argb:ee262626, argb:dd26ff26, argb:dd2626ff
! State:           'bg',     'fg',     'bgalt',  'hlbg',   'hlfg'
rofi.color-normal: argb:00262626, #a6a6a6, argb:00262626, argb:00262626, #ffffff

! Transparency
URxvt.depth: 32
URxvt.background: [65]#202020

Xft.dpi: 96
